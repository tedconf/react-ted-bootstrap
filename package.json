{
  "name": "@tedconf/react-ted-bootstrap",
  "author": "Jon Bellah <jon@ted.com>",
  "license": "MIT",
  "version": "0.6.1",
  "description": "A shared set of UI components for internal projects",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rimraf build && tsc",
    "deploy-storybook": "storybook-to-ghpages",
    "format": "prettier --write \"src/**/*.tsx\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm run lint",
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tedconf/react-ted-bootstrap.git"
  },
  "peerDependencies": {
    "emotion": ">= 10.0.5",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-info": "^5.0.3",
    "@storybook/addon-options": "^5.0.3",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.3",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/core-js": "^2.5.0",
    "@types/jest": "^24.0.0",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.8.3",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^4.1.1",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "6",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.0.0",
    "emotion": "^10.0.5",
    "jest": "^24.5.0",
    "jest-dom": "^3.0.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "prettier": "^1.16.1",
    "react": "^16.8.0",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.8.0",
    "react-scripts": "^3.0.0",
    "react-testing-library": "^7.0.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.0.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "preset": "ts-jest",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
